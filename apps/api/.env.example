# Oracle Database Connection (wallet-based mTLS)
DATABASE_URL=oracle+oracledb://ADMIN:your_password@solartrack_low
TNS_ADMIN=/path/to/your/Wallet_SolarTrack
WALLET_PASSWORD=your_wallet_password

# Supabase Auth (JWT validation only)
SUPABASE_JWT_SECRET=your_supabase_jwt_secret

# AI Provider (mock for testing, gemini for production)
DEFAULT_AI_PROVIDER=mock
GEMINI_API_KEY=your_gemini_api_key

# Environment (development or production)
ENVIRONMENT=development

# CORS (comma-separated list of allowed origins for production)
# CORS_ORIGINS=https://yourdomain.com,https://api.yourdomain.com
